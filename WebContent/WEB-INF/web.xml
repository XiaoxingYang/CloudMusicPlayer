<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <display-name>CourseWork</display-name>
 <servlet>
  <display-name>MusicManagementService</display-name>
  <servlet-name>MusicManagementService</servlet-name>
  <servlet-class>org.ds.coursework.service.MusicManagementService</servlet-class>
 </servlet>
 <servlet>
  <display-name>MusicStreamServer</display-name>
  <servlet-name>MusicStreamServer</servlet-name>
  <servlet-class>org.ds.coursework.service.MusicStreamServer</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>MusicManagementService</servlet-name>
  <url-pattern>/MusicManagementService</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>MusicStreamServer</servlet-name>
  <url-pattern>/MusicStreamServer</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>mainpage.jsp</welcome-file>

 </welcome-file-list>
 <login-config>
  <auth-method>BASIC</auth-method>
 </login-config>
</web-app>
